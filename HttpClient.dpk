package HttpClient;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$IMPLICITBUILD ON}

requires
  rtl,
  IndyCore,
  IndySystem,
  IndyProtocols,
  DesignIDE,
  dbrtl,
  dsnap,
  vcldb;

contains
  HttpClasses in 'HttpClasses.pas',
  HttpUtils in 'HttpUtils.pas',
  PropertyEditors in 'PropertyEditors.pas',
  UfrmHeadersEditor in 'UfrmHeadersEditor.pas' {FHeadersEditor};

end.



